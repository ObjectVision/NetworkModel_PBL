Template PerVetrekMoment_T
{
	parameter<uint32>   inTime;
	parameter<string>   inTime_string;
	///
	parameter<min_f>    Max_V_Time := max(/ModelParameters/Advanced/OV_Voortransport_Typen/MaxTime);
	
	unit<uint64> Within_VW_Window := select_with_org_rel(ToegestaneKetens/Result/From_Time_rel <= inTime + convert(Max_V_Time, s_f)[Time] + ModelParameters/Advanced/MaxWachttijdThuis)
	, Descr = "Opties binnen voortransport + wachten thuis window"
	{
		attribute<Euro>              Price                       := org_rel -> Result/Price;
		attribute<Euro>              Price_augm                  := org_rel -> Result/Price_augm;
		attribute<s_f>               Traveltime                  := org_rel -> Result/Traveltime;
		attribute<Toegestane_Ketens> Toegestane_Keten_rel        := org_rel -> Result/Toegestane_Keten_rel;
		
		attribute<c_Time_Places>     c_FromTime_Place_rel        := org_rel -> Result/c_FromTime_Place_rel;
		attribute<c_Time_Places>     c_ToTime_Place_rel          := org_rel -> Result/c_ToTime_Place_rel;
		attribute<Places>            From_Place_rel              := value(c_FromTime_Place_rel % uint64(#Places), Places);
		
		attribute<km>                TravelDist_Bus              := org_rel -> Result/TravelDist_Bus;
		attribute<km>                Traveldist_Metro            := org_rel -> Result/Traveldist_Metro;
		attribute<km>                Traveldist_Tram             := org_rel -> Result/Traveldist_Tram;
		attribute<km>                Traveldist_Rail             := org_rel -> Result/Traveldist_Rail;
		attribute<km>                Traveldist_Ferry            := org_rel -> Result/Traveldist_Ferry;
	}

	unit<uint64> Add_V_to_PT   := join_equal_values_uint64(V/To_Place_rel, Within_VW_Window/From_Place_rel)
	{
		attribute<V>                                           V_rel                            := first_rel; //debug
		attribute<Within_VW_Window>                            PT_rel                           := second_rel; //debug
		attribute<Places>                                      Place_rel                        := X_rel; //debug

		attribute<Time>                                        V_From_Time_rel                  := const(inTime[Time],V)[V_rel];
		attribute<Places>                                      V_From_Place_rel                 := V/From_Place_rel[V_rel];
		attribute<Time>                                        V_To_Time_rel                    := V_From_Time_rel + V_time[Time];
		
		attribute<c_Time_Places>                               V_c_fromTime_Place_rel           := combine_data(c_Time_Places, V_From_Time_rel, V_From_Place_rel);
		attribute<c_Time_Places>                               PT_c_fromTime_Place_rel          := Within_VW_Window/c_fromTime_Place_rel[PT_rel];
		attribute<c_Time_Places>                               PT_c_toTime_Place_rel            := Within_VW_Window/c_toTime_Place_rel[PT_rel];
		
		attribute<Time>                                        StartWacht_Time_rel              := V_To_Time_rel;
		attribute<Time>                                        EindWacht_Time_rel               := value(PT_c_fromTime_Place_rel / uint64(#Places), Time);
		
		attribute<s_f>                                         V_time                           := V/impedance[V_rel];
		attribute<string>                                      V_mode                           := V/Mode[V_rel];
		attribute<Time>                                        Wachttijd_Time_rel               := sub_or_null(EindWacht_Time_rel, StartWacht_Time_rel);
		attribute<s_f>                                         W_time                           := Wachttijd_Time_rel[s_f];
		
		attribute<c_Time_Places>                               c_fromTime_Place_rel             := V_c_fromTime_Place_rel;
		attribute<c_Time_Places>                               c_toTime_Place_rel               := PT_c_toTime_Place_rel;
		
		attribute<Euro>                                        V_price                          := V/Price[V_rel];

		attribute<Euro>                                        Price                            := Within_VW_Window/Price[PT_rel];
		attribute<Euro>                                        Price_augm                       := Within_VW_Window/Price_augm[PT_rel] + V_price;
		attribute<s_f>                                         PT_time                          := Within_VW_Window/Traveltime[PT_rel];
		attribute<Toegestane_Ketens>                           Toegestane_Keten_rel             := Within_VW_Window/Toegestane_Keten_rel[PT_rel];
		attribute<s_f>                                         Traveltime                       := (ModelParameters/Advanced/WachttijdThuisMeetellen * W_time) + V_time + PT_time;
		
		attribute<km>                                          TravelDist_Bus                   := Within_VW_Window/TravelDist_Bus[PT_rel];
		attribute<km>                                          Traveldist_Metro                 := Within_VW_Window/Traveldist_Metro[PT_rel];
		attribute<km>                                          Traveldist_Tram                  := Within_VW_Window/Traveldist_Tram[PT_rel];
		attribute<km>                                          Traveldist_Rail                  := Within_VW_Window/Traveldist_Rail[PT_rel];
		attribute<km>                                          Traveldist_Ferry                 := Within_VW_Window/Traveldist_Ferry[PT_rel];
		
		attribute<bool>                                        IsVTimeNotTooLong                := switch(
																										case(V_mode == 'W', V_time <= convert(ModelParameters/MaxWalkingTime_Org2Stops, s_f))
																										, case(V_mode == 'B', V_time <= convert(ModelParameters/MaxCyclingTime_Org2Stops, s_f))
																										, FALSE
																									);
		attribute<bool>                                        IsValid                          := Wachttijd_Time_rel <= ModelParameters_base/Advanced/MaxWachttijdThuis && IsVTimeNotTooLong;
		
		unit<uint64> Result := select_with_org_rel(IsValid)
		{
			attribute<c_Time_Places>               c_fromTime_Place_rel             := org_rel -> c_fromTime_Place_rel;
			attribute<c_Time_Places>               c_toTime_Place_rel               := org_rel -> c_toTime_Place_rel;
		
			attribute<s_f>                         W_time                           := org_rel -> W_time;
			attribute<s_f>                         V_time                           := org_rel -> V_time;
			attribute<string>                      V_mode                           := org_rel -> V_mode;
			
			attribute<Euro>                        Price                            := org_rel -> Price;
			attribute<Euro>                        Price_augm                       := org_rel -> Price_augm;
			attribute<s_f>                         PT_time                          := org_rel -> PT_time;
			attribute<Toegestane_Ketens>           Toegestane_Keten_rel             := org_rel -> Toegestane_Keten_rel;
			attribute<s_f>                         Traveltime                       := org_rel -> Traveltime;
			attribute<Places>                      To_Place_rel                     := value(c_ToTime_Place_rel % uint64(#Places), Places); 
			
			attribute<km>                          TravelDist_Bus                   := org_rel -> TravelDist_Bus;
			attribute<km>                          Traveldist_Metro                 := org_rel -> Traveldist_Metro;
			attribute<km>                          Traveldist_Tram                  := org_rel -> Traveldist_Tram;
			attribute<km>                          Traveldist_Rail                  := org_rel -> Traveldist_Rail;
			attribute<km>                          Traveldist_Ferry                 := org_rel -> Traveldist_Ferry;
		}
	}
	
	unit<uint64> Add_N_to_VPT   := join_equal_values_uint64(Add_V_to_PT/Result/To_Place_rel, N/From_Place_rel)
	{
		attribute<Add_V_to_PT/Result>                          PT_rel                           := first_rel; //debug
		attribute<N>                                           N_rel                            := second_rel; //debug
		attribute<Places>                                      Place_rel                        := X_rel; //debug
		
		attribute<c_Time_Places>                               PT_c_fromTime_Place_rel          := Add_V_to_PT/Result/c_fromTime_Place_rel[PT_rel];
		attribute<c_Time_Places>                               PT_c_toTime_Place_rel            := Add_V_to_PT/Result/c_toTime_Place_rel[PT_rel];
		attribute<Time>                                        PT_To_Time_rel                   := value(PT_c_toTime_Place_rel / uint64(#Places), Time);
		attribute<Time>                                        N_To_Time_rel                    := PT_To_Time_rel + N_time[Time];
		attribute<Places>                                      N_to_Place_rel                   := N/To_Place_rel[N_rel];
		attribute<s_f>                                         N_time                           := N/impedance[N_rel];
		attribute<string>                                      N_mode                           := N/Mode[N_rel];
		attribute<Euro>                                        N_price                          := N/Price[N_rel];
		
		attribute<c_Time_Places>                               c_fromTime_Place_rel             := PT_c_fromTime_Place_rel;
		attribute<c_Time_Places>                               c_toTime_Place_rel               := combine_data(c_Time_Places, N_To_Time_rel, N_to_Place_rel);
		
		attribute<Euro>                                        Price                            := Add_V_to_PT/Result/Price[PT_rel];
		attribute<Euro>                                        Price_augm                       := Add_V_to_PT/Result/Price_augm[PT_rel] + N_price;
		attribute<Toegestane_Ketens>                           Toegestane_Keten_rel             := Add_V_to_PT/Result/Toegestane_Keten_rel[PT_rel];
		attribute<s_f>                                         Traveltime                       := Add_V_to_PT/Result/Traveltime[PT_rel] + N_time;
		attribute<s_f>                                         W_time                           := Add_V_to_PT/Result/W_time[PT_rel];
		attribute<s_f>                                         V_time                           := Add_V_to_PT/Result/V_time[PT_rel];
		attribute<s_f>                                         PT_time                          := Add_V_to_PT/Result/PT_time[PT_rel];
		attribute<string>                                      V_mode                           := Add_V_to_PT/Result/V_Mode[PT_rel];
		
		attribute<km>                                          TravelDist_Bus                   := Add_V_to_PT/Result/TravelDist_Bus[PT_rel];
		attribute<km>                                          Traveldist_Metro                 := Add_V_to_PT/Result/Traveldist_Metro[PT_rel];
		attribute<km>                                          Traveldist_Tram                  := Add_V_to_PT/Result/Traveldist_Tram[PT_rel];
		attribute<km>                                          Traveldist_Rail                  := Add_V_to_PT/Result/Traveldist_Rail[PT_rel];
		attribute<km>                                          Traveldist_Ferry                 := Add_V_to_PT/Result/Traveldist_Ferry[PT_rel];
		
		attribute<bool>                                        IsNTimeNotTooLong                := switch(
																										  case(N_mode == 'W', N_time <= convert(ModelParameters/MaxWalkingTime_Stops2Dest, s_f))
																										, case(N_mode == 'B', N_time <= convert(ModelParameters/MaxCyclingTime_Stops2Dest, s_f))
																										, FALSE
																									);
		attribute<bool>                                        IsValid                          := Traveltime <= /ModelParameters/MaxTravelTime[s_f] && IsNTimeNotTooLong;
		unit<uint64> Result := select_with_org_rel(IsValid)
		{
			attribute<c_Time_Places>               c_fromTime_Place_rel             := org_rel -> c_fromTime_Place_rel;
			attribute<c_Time_Places>               c_toTime_Place_rel               := org_rel -> c_toTime_Place_rel;
		
			attribute<s_f>                         W_time                           := org_rel -> W_time, Descr = "Waiting at home";
			attribute<s_f>                         V_time                           := org_rel -> V_time;
			attribute<s_f>                         PT_time                          := org_rel -> PT_time;
			attribute<s_f>                         N_time                           := org_rel -> N_time;
			
			attribute<Euro>                        Price                            := org_rel -> Price;
			attribute<Euro>                        Price_augm                       := org_rel -> Price_augm;
			attribute<s_f>                         Traveltime                       := org_rel -> Traveltime;
			attribute<Toegestane_Ketens>           Toegestane_Keten_rel             := org_rel -> Toegestane_Keten_rel;
			attribute<string>                      V_mode                           := org_rel -> V_mode;
			attribute<string>                      N_mode                           := org_rel -> N_mode;
			
			attribute<km>                          TravelDist_Bus                   := org_rel -> TravelDist_Bus;
			attribute<km>                          Traveldist_Metro                 := org_rel -> Traveldist_Metro;
			attribute<km>                          Traveldist_Tram                  := org_rel -> Traveldist_Tram;
			attribute<km>                          Traveldist_Rail                  := org_rel -> Traveldist_Rail;
			attribute<km>                          Traveldist_Ferry                 := org_rel -> Traveldist_Ferry;
			
			// attribute<uint32>                      PriceTime_Score                  := =ModelParameters/MinimiseCriterium == 'Price'
																						// ? 'value(Price_augm * 100f, uint32) * value(ModelParameters/MaxTravelTime[s_f], uint32) + value(Traveltime, uint32)'
																						// : 'value(Traveltime * 10000f, uint32) + value(Price_augm * 100f, uint32)';
			// #include<OD_uq_attributen.dms>
			
			// unit<uint64>          OD_key_set    := combine_unit_uint64(uq_c_FromTime_Place, uq_c_ToTime_Place);
