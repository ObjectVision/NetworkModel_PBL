	}
	
	unit<uint64> Union_with_DirectOD   := union_unit_uint64(Add_N_to_VPT/Result, Direct_OD)
	{
			attribute<c_Time_Places>               c_fromTime_Place_rel             := union_data(., Add_N_to_VPT/Result/c_FromTime_Place_rel,  combine_data(c_Time_Places, inTime[Time], Direct_OD/From_Place_rel)); 
			attribute<c_Time_Places>               c_toTime_Place_rel               := union_data(., Add_N_to_VPT/Result/c_toTime_Place_rel, combine_data(c_Time_Places, inTime[Time] + Direct_OD/Duration_seconds[Time], Direct_OD/To_Place_rel)); 
		
			attribute<s_f>                         W_time                           := union_data(., Add_N_to_VPT/Result/W_time, const(0f, Direct_OD, s_f));
			attribute<s_f>                         V_time                           := union_data(., Add_N_to_VPT/Result/V_time,  const(0f, Direct_OD, s_f));
			attribute<s_f>                         PT_time                          := union_data(., Add_N_to_VPT/Result/PT_time, const(0f, Direct_OD, s_f));
			attribute<s_f>                         N_time                           := union_data(., Add_N_to_VPT/Result/N_time,  const(0f, Direct_OD, s_f));
			
			attribute<Euro>                        Price                            := union_data(., Add_N_to_VPT/Result/Price, Direct_OD/Price);
			attribute<Euro>                        Price_augm                       := union_data(., Add_N_to_VPT/Result/Price_augm, Direct_OD/Price);
			attribute<s_f>                         Traveltime                       := union_data(., Add_N_to_VPT/Result/Traveltime, Direct_OD/duration_seconds);
			attribute<Toegestane_Ketens>           Toegestane_Keten_rel             := union_data(., Add_N_to_VPT/Result/Toegestane_Keten_rel, const(null_u, Direct_OD, Toegestane_Ketens));
			attribute<string>                      V_mode                           := union_data(., Add_N_to_VPT/Result/V_mode, const('', Direct_OD));
			attribute<string>                      N_mode                           := union_data(., Add_N_to_VPT/Result/N_mode, const('', Direct_OD));
			
			attribute<km>                          TravelDist_Bus                   := union_data(., Add_N_to_VPT/Result/TravelDist_Bus, const(0f, Direct_OD, km));
			attribute<km>                          Traveldist_Metro                 := union_data(., Add_N_to_VPT/Result/Traveldist_Metro, const(0f, Direct_OD, km));
			attribute<km>                          Traveldist_Tram                  := union_data(., Add_N_to_VPT/Result/Traveldist_Tram, const(0f, Direct_OD, km));
			attribute<km>                          Traveldist_Rail                  := union_data(., Add_N_to_VPT/Result/Traveldist_Rail, const(0f, Direct_OD, km));
			attribute<km>                          Traveldist_Ferry                 := union_data(., Add_N_to_VPT/Result/Traveldist_Ferry, const(0f, Direct_OD, km));
			
			attribute<uint32>                      PriceTime_Score                  := =ModelParameters/MinimiseCriterium == 'Price'
																						? 'value(Price_augm * 100f, uint32) * value(ModelParameters/MaxTravelTime[s_f], uint32) + value(Traveltime, uint32)'
																						: 'value(Traveltime * 10000f, uint32) + value(Price_augm * 100f, uint32)';
																						
			#include<OD_uq_attributen.dms>
			
			unit<uint64>          OD_key_set    := combine_unit_uint64(uq_c_FromTime_Place, uq_c_ToTime_Place);
			attribute<OD_key_set> OD_key        := combine_data(OD_key_set, uq_c_FromTime_Place_rel, uq_c_ToTime_Place_rel);
			unit<uint64>          uq_OD_key     := unique(OD_key);
			attribute<uq_OD_key>  uq_OD_key_rel := rlookup(OD_key, uq_OD_key/values); //attribuut om partitionering van price/traveltime etc te doen.
	}
	
	unit<uint64> Result := Union_with_DirectOD/uq_OD_key
	{
		attribute<Union_with_DirectOD/uq_c_FromTime_Place>  uq_c_FromTime_Place_rel     := value(values / #Union_with_DirectOD/uq_c_ToTime_Place, Union_with_DirectOD/uq_c_FromTime_Place);
		attribute<Union_with_DirectOD/uq_c_ToTime_Place>    uq_c_ToTime_Place_rel       := value(values % #Union_with_DirectOD/uq_c_ToTime_Place, Union_with_DirectOD/uq_c_ToTime_Place);
		attribute<c_Time_Places>                            c_FromTime_Place_rel        := Union_with_DirectOD/uq_c_FromTime_Place/c_FromTime_Place_rel[uq_c_FromTime_Place_rel];
		attribute<c_Time_Places>                            c_ToTime_Place_rel          := Union_with_DirectOD/uq_c_ToTime_Place/c_ToTime_Place_rel[uq_c_ToTime_Place_rel];
		
		attribute<Union_with_DirectOD>                      Min_Index                   := min_index(Union_with_DirectOD/PriceTime_Score, Union_with_DirectOD/uq_OD_key_rel);

		attribute<Euro>                                     Price                       := Min_Index -> Price;
		attribute<Euro>                                     Price_augm                  := Min_Index -> Price_augm;
		attribute<s_f>                                      Traveltime                  := Min_Index -> Traveltime;
		attribute<Toegestane_Ketens>                        Toegestane_Keten_rel        := Min_Index -> Toegestane_Keten_rel;
		attribute<s_f>                                      W_time                      := Min_Index -> W_time, Descr = "Waiting at home";
		attribute<s_f>                                      V_time                      := Min_Index -> V_time;
		attribute<s_f>                                      PT_time                     := Min_Index -> PT_time;
		attribute<s_f>                                      N_time                      := Min_Index -> N_time;
		attribute<string>                                   V_mode                      := Min_Index -> V_mode;
		attribute<string>                                   N_mode                      := Min_Index -> N_mode;
		
		attribute<km>                                       TravelDist_Bus              := Min_Index -> TravelDist_Bus;
		attribute<km>                                       Traveldist_Metro            := Min_Index -> Traveldist_Metro;
		attribute<km>                                       Traveldist_Tram             := Min_Index -> Traveldist_Tram;
		attribute<km>                                       Traveldist_Rail             := Min_Index -> Traveldist_Rail;
		attribute<km>                                       Traveldist_Ferry            := Min_Index -> Traveldist_Ferry;
	}
}

