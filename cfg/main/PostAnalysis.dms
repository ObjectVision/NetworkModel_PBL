container PostAnalysis : using = "units;geometries"
{
	container ReadExport_PublicTransport_perRegio := 
		for_each_ne(
			/ModelParameters/Advanced/Regio/name
			, 'ReadExport_OV_T(''W_OV_W_with_WW'','+quote(ModelParameters/Advanced/Regio/name)+',''Ziekenhuizen_y2022_alg_ac_bp'')'
		);
	
	container Merge_Regions_PerTime :=
		for_each_ne(
			 ModelParameters/Advanced/MeasureMoments/name
			, 'Merge_Regions_T(''W_OV_W_with_WW'','+quote( ModelParameters/Advanced/MeasureMoments/name)+',''Ziekenhuizen_y2022_alg_ac_bp'')'
		);
	
	Template Merge_Regions_T
	{
		parameter<string> Export_CombinationTypes;
		parameter<string> Time;	
		parameter<string> Dest_str;
		//
		
		unit<uint32> Merged := ='union_unit('+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/domain')+')'
		, StorageName = "=Export_filename", StorageType = "gdalwrite.vect"
		{
			attribute<string>  OrgName    := ='union_data(.,'+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/data/OrgName')+')';
			attribute<string>  DestName   := ='union_data(.,'+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/data/DestName')+')';
			attribute<min_f>   Traveltime := ='union_data(.,'+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/data/Traveltime')+')';
			attribute<float32> Price      := ='union_data(.,'+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/data/Price[float32]')+')';
			attribute<string>  ModeUsed   := ='union_data(.,'+AsItemList('ReadExport_PublicTransport_perRegio/'+ModelParameters/Advanced/Regio/name+'/PerTime/'+Time+'/data/ModeUsed')+')';
			attribute<string>  Label      := OrgName +'_'+DestName, DialogType = "LabelText";
			
		}
		
		parameter<string> Export_filename       := ModelParameters/Advanced/fileNamePrefix+'Traveltime_Long_'+Export_CombinationTypes+''+fileNameSuffix+'.csv';
		parameter<string> fileNameSuffix :=  
			'_' + ModelParameters/Analysis_Date + 
			'_' + ModelParameters/Advanced/Arrival_or_Departure_at_time + 
			'_' + Time + 
			'_ORG-' + ModelParameters/Orgset + 
			'_DEST-'   + Dest_str;
	}
	
	Template ReadExport_OV_T
	{
		parameter<string> Export_CombinationTypes;
		parameter<string> Org_str;
		parameter<string> Dest_str;
		///
		
		container PerTime := 
			for_each_ne(
				ModelParameters/Advanced/MeasureMoments/name
				, 'ReadCSV_T('+quote(/ModelParameters/Advanced/MeasureMoments/name)+')'
			);
			
		Template ReadCSV_T
		{ 
			parameter<string> Time;	
			///
			parameter<string> filename       := ModelParameters/Advanced/fileNamePrefix+'Traveltime_Long_'+Export_CombinationTypes+''+fileNameSuffix+'.csv';
			
			parameter<string> fileNameSuffix :=  
				'_' + ModelParameters/Analysis_Date + 
				'_' + ModelParameters/Advanced/Arrival_or_Departure_at_time + 
				'_' + Time + 
				'_ORG-' + ModelParameters/Orgset + '-' + Org_str + 
				'_DEST-'   + Dest_str;
			
			unit<uint64>      domain         := range(uint64, 0[uint64], count_rows);
			parameter<string> fieldseparator := ';';
			parameter<string> filedata       :  StorageType   = "str",  StorageName = "=filename",  StorageReadOnly = "true";
			parameter<uint64> count_rows     := (strcount(filedata, fieldseparator) / strcount(headerline, fieldseparator))[uint64] - 1[uint64];
			parameter<string> headerline     := readLines(filedata, void, 0);
			
			unit<uint32> field := Range(uint32, 0, strcount(headerline, fieldseparator) + 1)
			{
				attribute<string> name := ReadArray(headerline , field, string, 0);
			}
				
			attribute<string> bodylines (domain) := readLines(filedata, domain, headerline/ReadPos);

			container data := 
				for_each_nedv(
					field/name
					,'ReadElems(
						BodyLines
						,string
						,'+ MakeDefined(field/name[sub_or_null(id(field),1)] + '/ReadPos','const(0, domain)')+'
						, 17
					)'
					,domain
					,string
				)
			{
				attribute<min_f>               Traveltime              (domain) := ='Traveltime_m_'+Time+'[float32] * 1[min_f]';
				attribute<string>              ModeUsed                (domain) := ='ModeUsed_'+Time;
				attribute<string>              OrgDestName             (domain) := OrgName + '_' + DestName;
			}
		}
	}
	
	
